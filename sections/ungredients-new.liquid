<section class="ungredients-new-section" 
         {% if section.settings.background_image %}
           style="background-image: url('{{ section.settings.background_image | img_url: '1920x' }}'); background-color: {{ section.settings.background_color | default: '#f58220' }};"
           data-desktop-bg="{{ section.settings.background_image | img_url: '1920x' }}"
         {% else %}
           style="background-color: {{ section.settings.background_color | default: '#f58220' }};"
         {% endif %}
         {% if section.settings.mobile_background_image %}
           data-mobile-bg="{{ section.settings.mobile_background_image | img_url: '990x' }}"
         {% endif %}>

  {% comment %} Main Content {% endcomment %}
  <div class="ungredients-new-container">
    <div class="ungredients-new-content">
      <div class="ungredients-new-box">
        
        {% comment %} Title with Icon {% endcomment %}
        <div class="ungredients-new-title-wrapper">
          <h2 class="ungredients-new-title">
            {{ section.settings.title | default: 'ungredients' }}
            <sup class="ungredients-supar-text">{{ section.settings.text }}</sup>
          </h2>
          
          {% if section.settings.icon_svg %}
            <div class="ungredients-new-icon">
              <img src="{{ section.settings.icon_svg | img_url: 'master' }}" 
                   alt="Icon" 
                   loading="lazy" 
                   aria-hidden="true">
            </div>
          {% endif %}
        </div>

        {% comment %} Subtitle {% endcomment %}
        {% if section.settings.subtitle != blank %}
          <div class="ungredients-new-subtitle">
            {{ section.settings.subtitle }}
          </div>
        {% endif %}

        {% comment %} Divider {% endcomment %}
        <div class="ungredients-new-divider"></div>

        {% comment %} Table {% endcomment %}
        <div class="ungredients-new-table-wrapper">
          <table class="ungredients-new-table">
            {%- for block in section.blocks -%}
              {%- if block.type == 'ungredient' -%}
                <tr {{ block.shopify_attributes }}>
                  <td>{{ block.settings.title | escape }}</td>
                  <td>{{ block.settings.description | escape }}</td>
                  <td><p>{{ block.settings.percentage | default: '0%' | escape }}</p></td>
                </tr>
              {%- endif -%}
            {%- endfor -%}
          </table>
        </div>

        {% comment %} Footer Text {% endcomment %}
        {% if section.settings.footer_text != blank %}
          <p class="ungredients-new-footer">{{ section.settings.footer_text | escape }}</p>
        {% endif %}

      </div>

      {% comment %} Button - Outside the box {% endcomment %}
      {% if section.settings.button_text != blank %}
        <div class="ungredients-new-button-wrapper">
          {% if section.settings.button_url != blank %}
            <a href="{{ section.settings.button_url }}" class="ungredients-new-button">
              <span>{{ section.settings.button_text | escape }}</span>
            </a>
          {% else %}
            <p class="ungredients-new-button button">
              <span>{{ section.settings.button_text | escape }}</span>
            </p>
          {% endif %}
        </div>
      {% endif %}
    </div>
  </div>

</section>

<script>
  (function() {
    const sections = document.querySelectorAll('.ungredients-new-section[data-mobile-bg]');
    if (!sections.length) return;
    
    function updateBackground(section) {
      const mobileBg = section.getAttribute('data-mobile-bg');
      if (!mobileBg) return;
      
      if (window.innerWidth <= 990) {
        section.style.backgroundImage = `url('${mobileBg}')`;
      } else {
        const desktopBg = section.getAttribute('data-desktop-bg');
        if (desktopBg) {
          section.style.backgroundImage = `url('${desktopBg}')`;
        }
      }
    }
    
    sections.forEach(function(section) {
      updateBackground(section);
    });
    
    let resizeTimer;
    window.addEventListener('resize', function() {
      clearTimeout(resizeTimer);
      resizeTimer = setTimeout(function() {
        sections.forEach(function(section) {
          updateBackground(section);
        });
      }, 100);
    });
  })();
</script>
  
