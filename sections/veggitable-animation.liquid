{% schema %}
{
  "name": "Veggie Hover Products",
  "tag": "section",
  "class": "section-veggie-hover",
  "max_blocks": 50,


  "settings": [
    { "type":"text","id":"title","label":"Heading","default":"we hide veggies in every bite" },
    { "type":"text","id":"subtitle","label":"Subtitle","default":"(so you don't have to)" },


    {
      "type":"select","id":"animation_style","label":"Hover Animation Style",
      "options":[
        { "value":"grow","label":"Grow" },
        { "value":"pop","label":"Pop" },
        { "value":"float","label":"Float" }
      ],
      "default":"grow"
    },


    { "type":"color_scheme","id":"color_scheme","label":"Color Scheme","default":"background-1" },


    { "type":"header","content":"Buttons" },
    { "type":"text","id":"button_label_1","label":"Button 1 Label","default":"Shop Now" },
    { "type":"url","id":"button_url_1","label":"Button 1 Link" },
    { "type":"text","id":"button_label_2","label":"Button 2 Label" },
    { "type":"url","id":"button_url_2","label":"Button 2 Link" }
  ],


  "blocks": [
    {
      "type": "product",
      "name": "Product Item",
      "settings": [
        { "type":"image_picker","id":"image","label":"Main Product Image" },
        { "type":"text","id":"title","label":"Product Title" },
        { "type":"textarea","id":"description","label":"Description" },


        { "type":"header","content":"Vegetable Group Image" },
        { "type":"image_picker","id":"veggie_group_image","label":"Vegetable Group Image (All vegetables in one image)" },
      ]
    }
  ],


  "presets": [
    { "name": "Veggie Hover Products" }
  ]
}
{% endschema %}



