{% comment %}
  Veggie Hover Product Section â€” Multi Image Upload Inside Block (FIXED VERSION)
{% endcomment %}


{{ 'section-veggie-hover.css' | asset_url | stylesheet_tag }}


<div class="veggie-hover-section color-{{ section.settings.color_scheme }} gradient">
  <div class="container">


    {% if section.settings.title %}
      <div class="section-heading center">
        <h2 class="veggie-title">{{ section.settings.title }}</h2>
        {% if section.settings.subtitle != blank %}
          <p class="veggie-subtitle">{{ section.settings.subtitle }}</p>
        {% endif %}
      </div>
    {% endif %}


    <div class="veggie-products-grid" data-animation="{{ section.settings.animation_style }}">


      {% for block in section.blocks %}
        {% assign index = forloop.index %}
      <div class="veggie-product-card card-{{ index }}"  {{ block.shopify_attributes }}>
        <div class="veggie-product-wrapper">


          <!-- ðŸŒ¿ GROUP VEGETABLE IMAGE -->
          {% if block.settings.veggie_group_image %}
            <div class="veggie-vegetables">
              <div class="veggie-group-image">
                <img src="{{ block.settings.veggie_group_image | image_url: width: 600 }}" loading="lazy" alt="Vegetables Group">
              </div>
            </div>
          {% endif %}


          <!-- MAIN PRODUCT IMAGE -->
          <div class="veggie-product-box">
            {% if block.settings.image %}
              <img
                src="{{ block.settings.image | image_url: width: 450 }}"
                loading="lazy"
                alt="{{ block.settings.title }}">
            {% else %}
              <div class="veggie-placeholder">
                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
              </div>
            {% endif %}
          </div>
        </div>


        <div class="veggie-product-info">
          {% if block.settings.title %}
            <h3 class="veggie-product-title">{{ block.settings.title }}</h3>
          {% endif %}
          {% if block.settings.description %}
            <p class="veggie-product-description">{{ block.settings.description }}</p>
          {% endif %}
        </div>
      </div>
      {% endfor %}


    </div>


    {% if section.settings.button_label_1 or section.settings.button_label_2 %}
      <div class="veggie-buttons">
        {% if section.settings.button_label_1 %}
          <a href="{{ section.settings.button_url_1 }}" class="button button--primary"><span>{{ section.settings.button_label_1 }}</span></a>
        {% endif %}
        {% if section.settings.button_label_2 %}
          <a href="{{ section.settings.button_url_2 }}" class="button button--secondary"><span>{{ section.settings.button_label_2 }}</span></a>
        {% endif %}
      </div>
    {% endif %}


  </div>
</div>




